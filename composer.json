{
  "name": "astrotomic/laravel-cachable-attributes",
  "type": "library",
  "description": "Allows to cache attribute accessor values in an easy way.",
  "keywords": [
    "laravel",
    "eloquent",
    "model",
    "cache",
    "attribute",
    "accessor",
    "getter"
  ],
  "homepage": "https://github.com/Astrotomic/laravel-cachable-attributes",
  "readme": "./README.md",
  "license": "MIT",
  "authors": [
    {
      "name": "Tom Witkowski",
      "email": "dev.gummibeer@gmail.com",
      "homepage": "https://gummibeer.de",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2",
    "illuminate/cache": "5.8.* || ^6.0"
  },
  "require-dev": {
    "illuminate/database": "5.8.* || ^6.0",
    "illuminate/support": "5.8.* || ^6.0",
    "orchestra/testbench": "3.8.* || ^4.0",
    "phpunit/phpunit": "^8.3"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Astrotomic\\CachableAttributes\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Astrotomic\\CachableAttributes\\Tests\\": "tests/"
    }
  },
  "prefer-stable": true,
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html=coverage"
  },
  "support": {
    "email": "dev.gummibeer@gmail.com",
    "issues": "https://github.com/Astrotomic/laravel-cachable-attributes/issues",
    "source": "https://github.com/Astrotomic/laravel-cachable-attributes"
  }
}
